# Windows Environment Configuration
# Copy this file to .env and update with your Windows machine IP

# Backend Configuration - UPDATE WITH YOUR WINDOWS IP
BACKEND_URL=https://192.168.1.100:3001
BACKEND_WS_URL=wss://192.168.1.100:3001

# NDI Bridge Configuration
NDI_BRIDGE_PORT=8000
NDI_BRIDGE_HOST=0.0.0.0
NDI_SOURCE_PREFIX=MobileCam
MAX_STREAMS=10
PROCESSING_QUALITY=high

# NDI SDK Configuration (Windows)
# Update this path to your NDI SDK installation
NDI_SDK_PATH=C:\Program Files\NewTek\NDI 5 SDK

# Logging
LOG_LEVEL=INFO
LOG_FILE=ndi_bridge.log

# Development
DEBUG=false
AUTO_CONSUME=true
